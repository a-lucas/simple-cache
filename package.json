{
  "name": "simple-url-cache",
  "version": "1.1.2",
  "description": "A simple cache engine used to store html document with URL keys",
  "main": "dist/simple-cache.min.js",
  "scripts": {
    "test": "./node_modules/.bin/_mocha",
    "typings": "./node_modules/.bin/typings install ",
    "build": "./node_modules/.bin/webpack",
    "doc": "node_modules/.bin/typedoc -out docs ts --mode modules --module commonjs --target ES5 --readme ./README.md --includes ./ts",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-lucas/simple-url-cache.git"
  },
  "keywords": [
    "node.js",
    "cache",
    "file caching",
    "url caching",
    "file cache",
    "url cache",
    "Redis",
    "FileStorage"
  ],
  "author": "Antoine lUCAS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-lucas/simple-cache/issues"
  },
  "homepage": "https://github.com/a-lucas/simple-cache#readme",
  "dependencies": {
    "redis": "^2.6.2",
    "shelljs": "^0.7.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "coveralls": "^2.11.12",
    "debug": "^2.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "pre-commit": "^1.1.3",
    "sinon-chai": "^2.8.0",
    "ts-loader": "^0.8.2",
    "typedoc": "^0.4.5",
    "typescript": "^1.8.10",
    "typings": "^1.3.3",
    "webpack": "^1.13.2"
  }
}
