{
  "name": "simple-url-cache",
  "version": "1.2.0",
  "description": "A simple cache engine used to store html document with URL keys",
  "main": "dist/simple-cache.min.js",
  "scripts": {
    "typings": "./node_modules/.bin/typings install ",
    "build": "./node_modules/.bin/webpack",
    "doc": "node_modules/.bin/typedoc -out docs ts --ignoreCompilerErrors --mode modules --module commonjs --target ES5 --readme ./README.md ",
    "test": "rm -Rf coverage ; ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/index.js",
    "codecov": "./node_modules/.bin/codecov"
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-lucas/simple-url-cache.git"
  },
  "keywords": [
    "node.js",
    "cache",
    "file caching",
    "url caching",
    "file cache",
    "url cache",
    "Redis",
    "FileStorage"
  ],
  "author": "Antoine lUCAS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-lucas/simple-cache/issues"
  },
  "homepage": "https://github.com/a-lucas/simple-cache#readme",
  "dependencies": {
    "es6-promise": "^3.3.1",
    "fs-extra": "^0.30.0",
    "redis": "^2.6.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "codecov": "^1.0.1",
    "debug": "^2.2.0",
    "es6-promise": "^3.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "pre-commit": "^1.1.3",
    "ts-loader": "^0.8.2",
    "typedoc": "^0.4.5",
    "typescript": "latest",
    "typings": "^1.3.3",
    "webpack": "^1.13.2"
  }
}
